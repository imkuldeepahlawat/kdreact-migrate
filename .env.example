# React Migration CLI Environment Configuration
# Copy this file to .env and modify the values as needed

# =============================================================================
# CLI Configuration
# =============================================================================

# Enable debug mode for verbose logging
DEBUG=false

# Set the default React version to migrate to (17, 18, or 19)
DEFAULT_REACT_VERSION=18

# Enable dry run mode by default (show changes without applying)
DRY_RUN=false

# Enable verbose output by default
VERBOSE=false

# =============================================================================
# Migration Options
# =============================================================================

# Skip dependency updates during migration
SKIP_DEPENDENCIES=false

# Only update dependencies, skip code transformations
DEPS_ONLY=false

# Skip configuration file updates
SKIP_CONFIG=false

# Skip backup creation (not recommended)
SKIP_BACKUP=false

# =============================================================================
# File Processing
# =============================================================================

# File extensions to process (comma-separated)
PROCESS_EXTENSIONS=.js,.jsx,.ts,.tsx

# Directories to exclude from processing (comma-separated)
EXCLUDE_DIRS=node_modules,dist,build,.git,.github

# Files to exclude from processing (comma-separated)
EXCLUDE_FILES=.env,.env.local,.env.production

# Maximum file size to process (in bytes)
MAX_FILE_SIZE=1048576

# =============================================================================
# Backup Configuration
# =============================================================================

# Backup directory name
BACKUP_DIR_NAME=.react-migration-backup

# Maximum number of backups to keep
MAX_BACKUPS=5

# Enable timestamped backups
TIMESTAMPED_BACKUPS=true

# =============================================================================
# Reporting Configuration
# =============================================================================

# Output format for reports (json, text, html)
REPORT_FORMAT=text

# Report file name
REPORT_FILE_NAME=migration-report

# Include detailed file changes in reports
DETAILED_REPORTS=true

# Save reports to file
SAVE_REPORTS=true

# =============================================================================
# Package Management
# =============================================================================

# Package manager to use (npm, yarn, pnpm)
PACKAGE_MANAGER=npm

# Registry URL for package installation
NPM_REGISTRY=https://registry.npmjs.org/

# Enable package manager cache
USE_CACHE=true

# =============================================================================
# Testing Configuration
# =============================================================================

# Run tests after migration
RUN_TESTS=true

# Test command to execute
TEST_COMMAND=npm test

# Test timeout in milliseconds
TEST_TIMEOUT=30000

# =============================================================================
# GitHub Actions Integration
# =============================================================================

# GitHub token for API access (for GitHub Actions workflows)
GITHUB_TOKEN=

# GitHub repository owner
GITHUB_OWNER=

# GitHub repository name
GITHUB_REPO=

# =============================================================================
# Documentation Generation
# =============================================================================

# Generate documentation after migration
GENERATE_DOCS=true

# Documentation output directory
DOCS_OUTPUT_DIR=docs

# Include private APIs in documentation
INCLUDE_PRIVATE_APIS=false

# =============================================================================
# Performance Configuration
# =============================================================================

# Number of concurrent file processing operations
CONCURRENT_FILES=4

# Memory limit for file processing (in MB)
MEMORY_LIMIT=512

# Enable progress indicators
SHOW_PROGRESS=true

# =============================================================================
# Error Handling
# =============================================================================

# Continue processing on errors
CONTINUE_ON_ERROR=false

# Log error details
LOG_ERRORS=true

# Error log file
ERROR_LOG_FILE=migration-errors.log

# =============================================================================
# Development Mode
# =============================================================================

# Enable development mode (additional logging, relaxed validation)
DEV_MODE=false

# Enable experimental features
EXPERIMENTAL_FEATURES=false

# Development server port (for local testing)
DEV_PORT=3000

# =============================================================================
# Security Configuration
# =============================================================================

# Enable security scanning of dependencies
SECURITY_SCAN=true

# Allow insecure dependencies (not recommended)
ALLOW_INSECURE_DEPS=false

# Validate package integrity
VALIDATE_PACKAGES=true

# =============================================================================
# Custom Transformations
# =============================================================================

# Custom transformation rules file
CUSTOM_TRANSFORMATIONS=

# Enable custom code transformations
ENABLE_CUSTOM_TRANSFORMATIONS=false

# Custom transformation timeout (in milliseconds)
TRANSFORMATION_TIMEOUT=5000

# =============================================================================
# Logging Configuration
# =============================================================================

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Log file path
LOG_FILE=migration.log

# Enable console logging
CONSOLE_LOGGING=true

# Enable file logging
FILE_LOGGING=false

# =============================================================================
# Network Configuration
# =============================================================================

# HTTP timeout for network requests (in milliseconds)
HTTP_TIMEOUT=30000

# Enable proxy support
USE_PROXY=false

# Proxy URL
PROXY_URL=

# Proxy authentication
PROXY_USERNAME=
PROXY_PASSWORD=

# =============================================================================
# Cache Configuration
# =============================================================================

# Enable caching of migration results
ENABLE_CACHE=true

# Cache directory
CACHE_DIR=.migration-cache

# Cache expiration time (in hours)
CACHE_EXPIRY=24

# =============================================================================
# Validation Configuration
# =============================================================================

# Validate project structure before migration
VALIDATE_PROJECT_STRUCTURE=true

# Check for required files
CHECK_REQUIRED_FILES=true

# Validate package.json structure
VALIDATE_PACKAGE_JSON=true

# =============================================================================
# Rollback Configuration
# =============================================================================

# Enable automatic rollback on failure
AUTO_ROLLBACK=false

# Rollback on test failure
ROLLBACK_ON_TEST_FAILURE=true

# Rollback on dependency installation failure
ROLLBACK_ON_DEPS_FAILURE=true

# =============================================================================
# Notifications
# =============================================================================

# Enable email notifications
EMAIL_NOTIFICATIONS=false

# Email configuration
EMAIL_HOST=
EMAIL_PORT=
EMAIL_USERNAME=
EMAIL_PASSWORD=
EMAIL_TO=

# Enable Slack notifications
SLACK_NOTIFICATIONS=false

# Slack webhook URL
SLACK_WEBHOOK_URL=

# =============================================================================
# Advanced Configuration
# =============================================================================

# Custom TypeScript configuration file
CUSTOM_TS_CONFIG=

# Custom ESLint configuration file
CUSTOM_ESLINT_CONFIG=

# Custom Jest configuration file
CUSTOM_JEST_CONFIG=

# Custom Babel configuration file
CUSTOM_BABEL_CONFIG=

# Custom Webpack configuration file
CUSTOM_WEBPACK_CONFIG=

# =============================================================================
# Experimental Features
# =============================================================================

# Enable React 19 experimental features
ENABLE_REACT_19_FEATURES=false

# Enable concurrent features
ENABLE_CONCURRENT_FEATURES=false

# Enable strict mode by default
ENABLE_STRICT_MODE=true

# Enable automatic code splitting
ENABLE_CODE_SPLITTING=false

# =============================================================================
# Migration Strategies
# =============================================================================

# Migration strategy (conservative, aggressive, custom)
MIGRATION_STRATEGY=conservative

# Enable incremental migration
INCREMENTAL_MIGRATION=false

# Migration batch size
MIGRATION_BATCH_SIZE=10

# Enable parallel migration
PARALLEL_MIGRATION=false

# =============================================================================
# Quality Assurance
# =============================================================================

# Run linting after migration
RUN_LINTING=true

# Run type checking after migration
RUN_TYPE_CHECK=true

# Run formatting after migration
RUN_FORMATTING=true

# Quality check timeout (in milliseconds)
QUALITY_CHECK_TIMEOUT=60000

# =============================================================================
# Integration Testing
# =============================================================================

# Run integration tests
RUN_INTEGRATION_TESTS=false

# Integration test timeout (in milliseconds)
INTEGRATION_TEST_TIMEOUT=120000

# Integration test environment
INTEGRATION_TEST_ENV=test

# =============================================================================
# Performance Monitoring
# =============================================================================

# Enable performance monitoring
ENABLE_PERFORMANCE_MONITORING=false

# Performance metrics file
PERFORMANCE_METRICS_FILE=performance-metrics.json

# Monitor memory usage
MONITOR_MEMORY_USAGE=false

# Monitor processing time
MONITOR_PROCESSING_TIME=true

# =============================================================================
# Custom Scripts
# =============================================================================

# Pre-migration script
PRE_MIGRATION_SCRIPT=

# Post-migration script
POST_MIGRATION_SCRIPT=

# Pre-test script
PRE_TEST_SCRIPT=

# Post-test script
POST_TEST_SCRIPT=

# =============================================================================
# Template Configuration
# =============================================================================

# Custom template directory
CUSTOM_TEMPLATE_DIR=

# Enable template customization
ENABLE_TEMPLATE_CUSTOMIZATION=false

# Template engine (handlebars, ejs, pug)
TEMPLATE_ENGINE=handlebars

# =============================================================================
# Plugin System
# =============================================================================

# Enable plugin system
ENABLE_PLUGINS=false

# Plugin directory
PLUGIN_DIR=plugins

# Load plugins automatically
AUTO_LOAD_PLUGINS=false

# =============================================================================
# API Configuration
# =============================================================================

# Enable API mode
API_MODE=false

# API port
API_PORT=3001

# API host
API_HOST=localhost

# Enable API authentication
API_AUTH=false

# API key
API_KEY=

# =============================================================================
# Database Configuration (for advanced features)
# =============================================================================

# Database URL
DATABASE_URL=

# Database type (sqlite, postgres, mysql)
DATABASE_TYPE=sqlite

# Enable database logging
DATABASE_LOGGING=false

# =============================================================================
# Monitoring and Analytics
# =============================================================================

# Enable analytics
ENABLE_ANALYTICS=false

# Analytics endpoint
ANALYTICS_ENDPOINT=

# Analytics key
ANALYTICS_KEY=

# Collect usage statistics
COLLECT_USAGE_STATS=false

# =============================================================================
# Support and Feedback
# =============================================================================

# Enable automatic error reporting
AUTO_ERROR_REPORTING=false

# Error reporting endpoint
ERROR_REPORTING_ENDPOINT=

# Enable feedback collection
ENABLE_FEEDBACK=false

# Feedback endpoint
FEEDBACK_ENDPOINT=

# =============================================================================
# Localization
# =============================================================================

# Default language
DEFAULT_LANGUAGE=en

# Enable internationalization
ENABLE_I18N=false

# Locale directory
LOCALE_DIR=locales

# =============================================================================
# Accessibility
# =============================================================================

# Enable accessibility features
ENABLE_ACCESSIBILITY=false

# Accessibility level (A, AA, AAA)
ACCESSIBILITY_LEVEL=AA

# =============================================================================
# Compliance
# =============================================================================

# Enable GDPR compliance
GDPR_COMPLIANCE=false

# Enable data retention policies
DATA_RETENTION=false

# Data retention period (in days)
DATA_RETENTION_DAYS=30

# =============================================================================
# Backup and Recovery
# =============================================================================

# Enable cloud backup
CLOUD_BACKUP=false

# Cloud backup provider (aws, gcp, azure)
CLOUD_BACKUP_PROVIDER=

# Cloud backup bucket
CLOUD_BACKUP_BUCKET=

# Cloud backup credentials
CLOUD_BACKUP_ACCESS_KEY=
CLOUD_BACKUP_SECRET_KEY=

# =============================================================================
# Security and Compliance
# =============================================================================

# Enable security scanning
SECURITY_SCANNING=false

