# Webhook secret
WEBHOOK_SECRET=

# Webhook events (migration_start, migration_complete, migration_failed)
WEBHOOK_EVENTS=migration_complete,migration_failed

# =============================================================================
# Customization and Extensions
# =============================================================================

# Custom configuration file
CUSTOM_CONFIG_FILE=

# Enable custom extensions
